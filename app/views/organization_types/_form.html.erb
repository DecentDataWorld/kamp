<%= simple_form_for(@organization_type, :html => { role: "form", multipart: true, class: "sky-form" }) do |f| %>  <%= f.error_notification %>

    <div class="form-group">
      <label for="name">Organization Type</label>
      <%= f.input_field :organization_type, class: "form-control", id: 'name', placeholder: 'Enter Type' %>
    </div>
    <div class="form-group">
      <%= f.button :submit %>
    </div>

<% end %>
